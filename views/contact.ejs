<%- include('header') %>
<body style="height:auto;">
        <section>
            <div class="col-md-12">
                <%- include('navbar') %>
                
                <div class="container-fluid" style="border-radius: 3rem; margin-top: 80px;">
                    <div id="carouselExampleFade" class="carousel slide carousel-fade" data-bs-ride="carousel" >
                      <div class="carousel-inner">
                        <div class="carousel-item active">
                          <img src="image/aboutus.jpg" style="border-radius: 3rem;" class="d-block w-100 mt-3" alt="...">
                        </div>
                        <div class="carousel-item">
                          <img src="image/office.png" style="border-radius: 3rem;" class="d-block w-100 mt-3" alt="...">
                        </div>
                        <div class="carousel-item">
                          <img src="image/office1.png" style="border-radius: 3rem;" class="d-block w-100 mt-3" alt="...">
                        </div>
                      </div>
                      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                      </button>
                      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                      </button>
                    </div>
                  </div>
    
                  <div class="card mt-5" style="width: 70%; margin-left: 15%; border-radius: 2rem; background-color: aliceblue;">
                    <div class="container mt-5" style="text-align: center">
              
                      
              
                      <form class="center">
                        <div class="form-row">
                          <div class="form-group col-md-6">
                            <!-- <label for="inputEmail4">Email</label> -->
                            <input
                              type="text"
                              class="form-control mt-3"
                              id="Name"
                              placeholder="Name"
                            />
                          </div>
                          <div class="form-group col-md-6">
                            <!-- <label for="inputPassword4">Password</label> -->
                            <input
                              type="email"
                              class="form-control mt-3"
                              id="email"
                              placeholder="Email"
                            />
                          </div>
                        </div>
                        <div class="form-group col-md-6">
                          <!-- <label for="inputAddress">Address</label> -->
                          <input
                            type="password"
                            class="form-control mt-3"
                            id="password"
                            placeholder="Password"
                          />
                        </div>
              
                        <div class="form-group col-md-6">
                          <!-- <label for="inputAddress">Address</label> -->
                          <input
                            type="password"
                            class="form-control mt-3"
                            id="confirm-password"
                            placeholder="Confirm Password"
                          />
                        </div>
              
                        <input type="checkbox" onclick="myFunction()" ><span>&nbsp;Show Password</span>
                        <div class="form-group col-md-6 my-2">
                          <!-- <label class="form-label">Profile Picture</label> -->
                          <input
                            type="file"
                            name="profile_image"
                            class="form-control"
                            required
                          />
                        </div>
              
                        <div class="form-group">
                          <!-- <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="gridCheck">
                    <label class="form-check-label" for="gridCheck">
                      Check me out
                    </label>
                  </div> -->
                        </div>
                        <button type="submit" class="btn btn-primary col-md-6 mt-3" style="margin-bottom: 20px;">
                          Sign in
                        </button>
                      </form>
                      <!-- <span class=" text-center pt-3">HAVE AN ACCOUNT?
                        <a style="color: #393f81;" href="/login.html">LOGIN NOW</a>
                    </span> -->
                    </div>
                  </div>

                    <%- include('footer') %>
            </div>
        </section>
    </body>
    <%- include('script') %>
  </html>